<!DOCTYPE html>
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Курсовая работа Мускатин</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Запросы", "Index", "requests")</li>
                    <li>@Html.ActionLink("Администраторы", "Index", "admins")</li>
                    <li>@Html.ActionLink("Пользователи", "Index", "users")</li>
                    <li>@Html.ActionLink("Подразделения", "Index", "podrs")</li>
                    <li>@Html.ActionLink("Статус", "Index", "status_req")</li>
                    <li>@Html.ActionLink("Запрос 1", "Index", "quest1")</li>
                    <li>@Html.ActionLink("Запрос 2", "Index", "quest2")</li>
                </ul>
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - мой курсовой проект</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
