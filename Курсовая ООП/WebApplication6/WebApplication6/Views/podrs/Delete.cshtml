@model WebApplication6.Models.podr

@{
    ViewBag.Title = "Удаление подразделения";
}

<h2>Удаление подразделения</h2>

<h3>Вы хотите удалить данное подразделение?</h3>
<div>
    <h4>Подразделение</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>

            @Html.DisplayNameFor(model => model.namePodr)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.namePodr)
           
        </dd>
    </dl>
    <dl class="dl-horizontal">
        
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Удалить" class="btn btn-default" /> |
            @Html.ActionLink("Назад", "Index")
        </div>
    }
</div>
